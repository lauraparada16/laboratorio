<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">
    <display-name>CrunchifyJSPServletExample</display-name>
    
    <servlet>
        <servlet-name>IniciarSesion</servlet-name>
        <servlet-class>com.crunchify.jsp.servlet.IniciarSesion</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ServletFactura</servlet-name>
        <servlet-class>com.crunchify.jsp.servlet.ServletFactura</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ServletInventario</servlet-name>
        <servlet-class>com.crunchify.jsp.servlet.ServletInventario</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ServletPersona</servlet-name>
        <servlet-class>com.crunchify.jsp.servlet.ServletPersona</servlet-class>
    </servlet>
    
     <servlet-mapping>
        <servlet-name>IniciarSesion</servlet-name>
        <url-pattern>/IniciarSesion</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ServletFactura</servlet-name>
        <url-pattern>/ServletFactura</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ServletInventario</servlet-name>
        <url-pattern>/ServletInventario</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ServletPersona</servlet-name>
        <url-pattern>/ServletPersona</url-pattern>
    </servlet-mapping>

</web-app>
